      },
      "googleServicesFile": "./google-services.json",
      "package": "com.bigwave1",
      "versionCode": 23
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "@react-native-firebase/app",
      ["expo-notifications", {
        "color": "#c8c8c8"
      }]
  
    ],
    "extra": {
      "eas": {
        "projectId": "24ce9713-10d0-4ee6-8a7e-4ebd8f4ed954"
